<ddObj Name="Variables" Kind="VariableGroup">
<ddObj Name="TRQ_DES_Throttle_Input" Kind="Variable">
   <ddProperty Name="Description">Throttle input value after selection of input channel [%]; Limits: 0...100</ddProperty>
   <ddProperty Name="Type">Float32</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">100</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">s</ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_Brake_Input" Kind="Variable">
   <ddProperty Name="Description">Brake input value after selection of input channel [%]; Limits: -100...0</ddProperty>
   <ddProperty Name="Type">Float32</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">-100</ddProperty>
   <ddProperty Name="Max">0</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">s</ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_ReverseGear_Input" Kind="Variable">
   <ddProperty Name="Description">Shows if reverse gear is selected after selection of input channel;  0 = forward gear selected; 1 = reverse gear selected</ddProperty>
   <ddProperty Name="Type">Float32</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">1</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">s</ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_TorqueRequest" Kind="Variable">
   <ddProperty Name="Description">Shows the desired torque request returned to trqdesApi [%]; Limits: -100...100</ddProperty>
   <ddProperty Name="Type">Float32</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>s
   <ddProperty Name="Min">-100</ddProperty>
   <ddProperty Name="Max">100</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">%</ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_TorqueRequest_UpperLim" Kind="Variable">
   <ddProperty Name="Description">Shows if desired torque has reached the upper bound of allowed operational range</ddProperty>
   <ddProperty Name="Type">UInt8</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">1</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit"></ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_TorqueRequest_LowerLim" Kind="Variable">
   <ddProperty Name="Description">Shows if desired torque has reached the lower bound of allowed operational range</ddProperty>
   <ddProperty Name="Type">UInt8</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">1</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit"></ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_HillAssist_State" Kind="Variable">
   <ddProperty Name="Description">Shows the actual state of hill-assist algorithm; 0 = Initial state; 1 = Throttle priorization; 2 = Accelerate without priorization; </ddProperty>
   <ddProperty Name="Type">UInt8</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">4</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit"></ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_HillAssist_ValCounter" Kind="Variable">
   <ddProperty Name="Description">Shows the actual value of hill-assist counter to priorize throttle</ddProperty>
   <ddProperty Name="Type">UInt16</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Value"></ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">65535</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit"></ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_C_ReverseGear_TestInput" Kind="Variable">
   <ddProperty Name="Description">Test parameter for manual input of reverse gear signal [-]; Limits: 0...1</ddProperty>
   <ddProperty Name="Type">UInt8</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>s
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">1</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit"></ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_C_ThrottlePriorization_Time" Kind="Variable">
   <ddProperty Name="Description">Parameter for time during which throttle will be priorized when both brake and throttle pedal are used in parallel [ms]; Limits: 0...65535</ddProperty>
   <ddProperty Name="Type">UInt16</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Min">0</ddProperty>
   <ddProperty Name="Max">65535</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">ms</ddProperty>
   </ddObj>
</ddObj>
<ddObj Name="TRQ_DES_C_ThrottlePriorization_MaxRotorSpeed" Kind="Variable">
   <ddProperty Name="Description">Parameter for maximum rotor speed to priorize throttle when both brake and throttle pedal are used in parallel [1/s]; Limits: -1...2000</ddProperty>
   <ddProperty Name="Type">Float32</ddProperty>
   <ddProperty Name="Scaling">./LocalScaling</ddProperty>
   <ddProperty Name="Min">-1</ddProperty>
   <ddProperty Name="Max">2000</ddProperty>
   <ddProperty Name="Address"></ddProperty>
   <ddObj Name="LocalScaling" Kind="Scaling">
      <ddProperty Name="LSB">1</ddProperty>
      <ddProperty Name="Unit">1/s</ddProperty>
   </ddObj>
</ddObj>
</ddObj>